:5:{s:4:"data";s:70:"tag:blogger.com,1999:blog-4880198594761104421.post-9113189620897963186";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:9:"published";a:1:{i:0;a:5:{s:4:"data";s:29:"2010-03-18T19:43:00.000-07:00";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"updated";a:1:{i:0;a:5:{s:4:"data";s:29:"2010-05-15T12:46:35.929-07:00";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:38:"Inner Peace Tool ~ Reiki Healing Photo";s:7:"attribs";a:1:{s:0:"";a:1:{s:4:"type";s:4:"text";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"content";a:1:{i:0;a:5:{s:4:"data";s:1863:"While you look at the picture, know that the Intention is for the  healing of any ill you may have, be it physical, mental or Spiritual.  Yes...this is for You and only You, that is my Intention. As this photo was taken, I was sending out Reiki Healing Energy to anyone who sees it. You have  found your way to this Healing picture, and since I don't believe in  coincidences or accidents, I am glad you are here, in the Now with me.<br /><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/_BZvDzUC5las/S6LnCEExyFI/AAAAAAAAAHU/nGa0MjvGgrg/s1600-h/AnneSends.jpg" imageanchor="1" style="clear: left; float: left; margin-bottom: 1em; margin-right: 1em;"><img border="0" height="343" src="http://2.bp.blogspot.com/_BZvDzUC5las/S6LnCEExyFI/AAAAAAAAAHU/nGa0MjvGgrg/s400/AnneSends.jpg" width="400" /></a></div><br /><div class="separator" style="clear: both; text-align: center;"></div>Take a deep breath and exhale out your nose. One more, in through your nose then out through your nose. Uncross your arms, legs and ankles. You may place your hands on your legs, palms facing up.<br /><br />Now relax your forehead...relax your shoulders...feel your weight supported in the chair.<br /><br />Relax your eyebrows, let them be heavy over your eyes....loosen your jaw....and finally, relax your tongue inside your mouth. (This relaxes your nervous system and can be done anywhere, anytime).<br /><br />Visualize yourself in perfect health, your body in perfect glowing health...see your mind centered and at peace. See the most perfect version of yourself. Make it your Intention to receive Healing now.<br /><br />And so it is.<div class="blogger-post-footer"><img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/4880198594761104421-9113189620897963186?l=myblackfeather.blogspot.com' alt='' /></div>";s:7:"attribs";a:1:{s:0:"";a:1:{s:4:"type";s:4:"html";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:3:{i:0;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:3:{s:3:"rel";s:4:"edit";s:4:"type";s:20:"application/atom+xml";s:4:"href";s:82:"http://www.blogger.com/feeds/4880198594761104421/posts/default/9113189620897963186";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:1;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:3:{s:3:"rel";s:4:"self";s:4:"type";s:20:"application/atom+xml";s:4:"href";s:82:"http://www.blogger.com/feeds/4880198594761104421/posts/default/9113189620897963186";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:2;a:5:{s:4:"data";s:0:"";s:7:"attribs";a:1:{s:0:"";a:4:{s:3:"rel";s:9:"alternate";s:4:"type";s:9:"text/html";s:4:"href";s:84:"http://myblackfeather.blogspot.com/2010/03/inner-peace-tool-reiki-healing-photo.html";s:5:"title";s:38:"Inner Peace Tool ~ Reiki Healing Photo";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:6:"author";a:1:{i:0;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:27:"http://www.w3.org/2005/Atom";a:3:{s:4:"name";a:1:{i:0;a:5:{s:4:"data";s:8:"Anneland";s:7:"attribs